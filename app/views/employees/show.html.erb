<h1><%=@employee.first_name + " " + @employee.last_name%></h1>

<ul>
<h3>Alias: <%=@employee.alias%></h3>
<h3>Office: <%=@employee.office%></h3>
<h3>Title: <%=@employee.title%></h3>
<h3>Picture: <img src=<%=@employee.img_url%>></h3>
<h3>Dogs:</h3>
<li><%=@employee.dog.name%></li>
</ul>

<%= button_to "FIRE EMPLOYEE", employee_path(@employee), method: :delete %>