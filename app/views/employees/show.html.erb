<h1> <%= @employee.first_name %> <%= @employee.last_name %>'s Profile </h1>
<img src= "<%= @employee.img_url %>" >
<ul>
    <li> Alias: <%= @employee.alias %> </li>
    <li> Title: <%= @employee.title %> </li>
    <li> Office: <%= @employee.office %> </li>
    <li> Dog: <%= @employee.dog.name %> </li>
</ul>

<%= button_to "Edit This Employee", edit_employee_path(@employee), method: :get %>