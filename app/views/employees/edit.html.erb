<h1>Employee Form</h1>
<br>
<%= form_for @employee do |f| %>
<%=f.label "First Name"%>
<%=f.text_field :first_name%>
<br>
<%=f.label "Last Name"%>
<%=f.text_field :last_name%>
<br>
<%=f.label "Alias"%>
<%=f.text_field :alias%>
<br>
<%=f.label "Office Location"%>
<%=f.text_field :office%>
<br>
<%=f.label "Job Title"%>
<%=f.text_field :title%>
<br>
<%=f.label "Image Url"%>
<%=f.text_field :img_url%>
<br>
<%=f.collection_select :dog_id, @dogs, :id, :name%>
<br>
<%=f.submit "Update Employee"%>
<%end%>

<% if flash[:errors] %>
<% flash[:errors].each do |message| %>
<p><%= message %></p>
<%end%>
<%end%>